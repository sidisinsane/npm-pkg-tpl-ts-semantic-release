{
  "name": "@humanuals/npm-pkg-tpl-ts-semantic-release",
  "version": "0.0.1",
  "description": "A npm package template for typescript using semantic-release for automating the package release workflow.",
  "keywords": [
    "github",
    "npm",
    "boilerplate",
    "template",
    "typescript",
    "tsup",
    "prettier",
    "semantic-release"
  ],
  "homepage": "https://github.com/sidisinsane/npm-pkg-tpl-ts-semantic-release#readme",
  "bugs": {
    "url": "https://github.com/sidisinsane/npm-pkg-tpl-ts-semantic-release/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sidisinsane/npm-pkg-tpl-ts-semantic-release.git"
  },
  "license": "MIT",
  "author": {
    "name": "Dirk Sidney Jansen",
    "email": "sidisinsane@users.noreply.github.com",
    "url": "https://github.com/sidisinsane"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "docs": "typedoc",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "git:commit": "cz",
    "lint": "tsc",
    "release": "pnpm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@mxssfd/typedoc-theme": "~1.1.3",
    "@semantic-release/changelog": "~6.0.3",
    "@semantic-release/git": "~10.0.1",
    "@types/jest": "~29.5.4",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "~29.6.4",
    "prettier": "~3.0.3",
    "semantic-release": "~21.1.1",
    "ts-jest": "~29.1.1",
    "tsup": "~7.2.0",
    "typedoc": "~0.25.1",
    "typescript": "~5.2.2"
  },
  "packageManager": "pnpm@8.7.4"
}
